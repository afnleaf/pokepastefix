  {
  "name": "pokepastefix",
  "version": "0.1.4",
  "manifest_version": 3,
  "description": "Fix missing pictures on pokepaste.",
  "permissions": [
    "tabs"
  ],
  "host_permissions": ["https://pokepast.es/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://pokepast.es/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["https://pokepast.es/*"],
      "resources": ["/images/*"]
    }
  ]
}