{
  "name": "pokepastefix",
  "version": "1.0.3",
  "manifest_version": 3,
  "description": "Fix missing pictures on pokepaste.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://pokepast.es/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "128": "icon128.png"
  }
}
